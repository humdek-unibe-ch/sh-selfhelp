{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "file:///schemas/json-logic/operators/logic/notEqual.json",

  "title": "Not-Equal",
  "description": "Tests not-equal, with type coercion. Requires two arguments.",

  "type": "object",
  "additionalProperties": false,
  "required": ["!="],
  "properties": {
    "!=": {
      "$ref": "../../common/binary-args.json"
    }
  },
  "examples": [
    { "!=": [1, 2] },
    { "!=": [1, "1"] }
  ]
}
