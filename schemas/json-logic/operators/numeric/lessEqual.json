{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "file:///schemas/json-logic/operators/numeric/lessEqual.json",

  "title": "<=",
  "description": "Less than or equal to. You can use a special case of <= to test that one value is inclusively between two others.",

  "type": "object",
  "additionalProperties": false,
  "required": ["<="],
  "properties": {
    "<=": {
      "$ref": "../../common/trinary-args.json"
    }
  },
  "examples": [
    { "<=": [1, 1] },
    { "<=": [1, 2, 3] },
    { "<=": [0, {"var":"temp"}, 100] }
  ]
}
