{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "Genders List Response",
  "description": "Response schema for genders list",
  "properties": {
    "genders": {
      "type": "array",
      "description": "List of genders",
      "items": {
        "type": "object",
        "properties": {
          "id": { "type": "integer", "description": "Gender ID" },
          "name": { "type": "string", "description": "Gender name" }
        },
        "required": ["id", "name"],
        "additionalProperties": false
      }
    }
  },
  "required": ["genders"],
  "additionalProperties": false
}


