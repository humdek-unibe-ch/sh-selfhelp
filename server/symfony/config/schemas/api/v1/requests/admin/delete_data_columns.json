{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "Delete Data Columns Request",
  "description": "Schema for deleting selected columns from a data table",
  "required": ["columns"],
  "properties": {
    "columns": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "string",
        "minLength": 1,
        "maxLength": 255
      }
    }
  },
  "additionalProperties": false
}


